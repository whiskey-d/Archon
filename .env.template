# Supabase Configuration
SUPABASE_URL=http://YOUR_SUPABASE_IP:8000
SUPABASE_SERVICE_KEY=your-supabase-service-role-jwt-token-here

# AI API Keys
ANTHROPIC_API_KEY=your-anthropic-api-key-here
OPENAI_API_KEY=

# Optional Services
LOGFIRE_TOKEN=
GITHUB_PAT_TOKEN=

# Service Ports
ARCHON_SERVER_PORT=8181
ARCHON_MCP_PORT=8051
ARCHON_AGENTS_PORT=8052
AGENT_WORK_ORDERS_PORT=8053
ARCHON_UI_PORT=3737

# Host Configuration
HOST=0.0.0.0
ARCHON_HOST=localhost

# Feature Flags
AGENTS_ENABLED=false
ENABLE_AGENT_WORK_ORDERS=true
PROD=false
VITE_SHOW_DEVTOOLS=false

# Logging
LOG_LEVEL=INFO

# Service Discovery
SERVICE_DISCOVERY_MODE=docker_compose
STATE_STORAGE_TYPE=supabase
TRANSPORT=sse

# Docker Environment
DOCKER_ENV=true
